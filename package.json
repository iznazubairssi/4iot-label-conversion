{
  "name": "4iot-label-conversion",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@azure/storage-blob": "^12.29.1",
    "@cap-js/postgres": "^2",
    "@sap/cds": "^9",
    "@sap/cds-mtxs": "^3",
    "@sap/xssec": "^4",
    "express": "^4",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "cds-plugin-ui5": "^0.13.0",
    "rimraf": "^5.0.5",
    "mbt": "^1.2.29"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-project1": "cds watch --open label.conversion.project1/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy 4iot-label-conversion --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "private": true,
  "cds": {
    "requires": {
      "[production]": {
        "auth": "xsuaa",
        "multitenancy": true
      },
      "[with-mtx]": {
        "multitenancy": true
      },
      "destinations": true,
      "html5-repo": true
    },
    "profile": "with-mtx-sidecar"
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/project1"
  ]
}
